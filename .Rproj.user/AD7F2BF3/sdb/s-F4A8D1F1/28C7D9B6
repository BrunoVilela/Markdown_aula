{
    "contents" : "churrasco<-function(mulheres,homens,bebidapropria=0,Hnaobebe=0,Mnaobebe=0,Hveget=0,Mveget=0,boi=15,linguica=10,queijo=7,Pboi=0.7,Plinguica=0.2,Pqueijo=0.1,cerveja=2,FomeH=0.6,FomeM=0.4,SedeH=8,SedeM=5,penetra=FALSE,Hdisp=-1,Mdisp=-1,Extras=(homens+mulheres)*1.5,refrigerante=5,suco=0,Vsuco=3.50, Decoracao=0,divisao=\"por pessoa\",alunos.de.pos.graduacao.na.aula.do.r.em.pirenopolis=FALSE,patrocinio=0){\n  \n  #Sede medida em latas\n  #Fome medida em Kg\n  #Carne=preço da carne em Kg\n  #Cerveja=preço da cerveja em latas\n  #Refrigerante 2L\n  ##############################################\n  \n  #Penetras aumentam em 10% a galera do churrasco\n  if (penetra==TRUE){\n    homens<-ceiling(c(homens+(homens*(0.10))))\n    mulheres<-ceiling(c(mulheres+(mulheres*(0.10))))}\n  \n  #Os vegetarianos estao em funçao do queijo e diminuem a linguica e boi, e dos extras\n  carnivoros<-((homens+mulheres)-(Hveget+Mveget))/(homens+mulheres)\n  Pboi<- Pboi*carnivoros\n  Plinguica<-Plinguica*carnivoros\n  Pqueijo<- 1-(Pboi+Plinguica)\n  carne<-boi*Pboi+linguica*Plinguica+queijo*Pqueijo\n  Extras<-Extras+(Hveget+Mveget)*2\n  if(carnivoros<0.5){warning(\"Muito vegetariano, melhor fazer outro tipo de confraternização...\")}\n    \n\n  \n  #Quantidade de carne\n  Qcarne=((FomeM)*(mulheres-Mveget))+((FomeH)*(homens-Hveget))\n  Qlinguica=round(Plinguica*Qcarne,1)\n  Qboi=round(Pboi*Qcarne,1)\n  Qqueijo=round(Pqueijo*Qcarne,1)\n  \n  #Quantidade de cerveja\n  Qcerveja=((SedeH)*(homens-Hnaobebe))+((SedeM)*(mulheres-Mnaobebe))\n  \n  #Quantidade de refrigerante\n  Refri<-ceiling((Hnaobebe+Mnaobebe-suco-bebidapropria)*0.5)\n  \n  #Custo refrigerante\n  Prefri<-Refri*refrigerante\n  #Custo do suco\n  Psuco<-suco*Vsuco\n          \n  #Custo total dos homens\n  valorH=((FomeH)*(homens-Hveget)*carne)+(((SedeH)*(homens-Hnaobebe))*cerveja)+Extras/homens+Decoracao/homens+Prefri/homens+Psuco/homens-patrocinio/homens\n  #Custo total das mulheres\n  valorM=((FomeM)*(mulheres-Mveget)*carne)+(((SedeM)*(mulheres-Mnaobebe))*cerveja)+Extras/mulheres+Decoracao/mulheres+Prefri/mulheres+Psuco/mulheres-patrocinio/homens\n  \n  #valor por pessoa\n  valor=ceiling((valorH+valorM)/(mulheres+homens))\n  #Valor homens \n  valorhp=ceiling(valorH/homens)\n  #Valor mulheres\n  valormp=ceiling(valorM/mulheres)\n  \n  #valor quem nao bebe cerveja\n  PPP<-patrocinio/(homens+mulheres)\n  valorsembebe=ceiling((((carne*Qcarne)+Extras+Decoracao)/(homens+mulheres)))\n  valorcombebe=ceiling(valorsembebe+((cerveja*Qcerveja)/(homens+mulheres-Hnaobebe-Mnaobebe)))\n  valornaobebe=ceiling(valorsembebe+((Prefri+Psuco)/(Mnaobebe+Hnaobebe-bebidapropria)))\n  if((Hnaobebe+Mnaobebe)==0){\n  valornaobebe=\"não há pessoas que não bebem\"}\n  if(bebidapropria==0){\n    valorsembebe=\"não há pessoas que levarão a própria bebida\"}\n  \n  #valor total \n  valortotal=(carne*Qcarne)+(cerveja*Qcerveja)+Extras+Decoracao+Prefri\n  \n  \n  #Milagre da vida\nif(Hdisp==-1){Hdisp<-ceiling(homens*(0.9))\n}\nif(Mdisp==-1){Mdisp<-ceiling(mulheres*(0.5))\n}\n  Sedarbem=ceiling((SedeM*0.025)+0.2*Hdisp)\n  if (Mdisp>=Sedarbem){casais<-Mdisp}\n  else {casais<-Sedarbem}\n  finalmentes<-casais*0.1\n  aticoncep<-finalmentes*0.05\n  Milagre<-(aticoncep*0.2)*100\n  if(alunos.de.pos.graduacao.na.aula.do.r.em.pirenopolis==TRUE){Milagre<-sample(50:100,1)}\n  \n  \n  if(homens/mulheres>1.7){(warning(\"CUIDADO: Muito homem, churrasco do CTG!\"))}\n  \n  \nif(divisao==\"por pessoa\"){ \nreturn(list(\"valor total\"=valortotal, \"valor por pessoa\"=valor,\"quantidade de cerveja(latas)\"=Qcerveja, \"quantidade de refrigerante\"=Refri,\"quantidade de suco\"=suco, \"quantidade de carne de boi (kg)\"=Qboi,\"quantidade de linguiça (kg)\"=Qlinguica, \"quantidade de queijo (kg)\"=Qqueijo, \"CHANCE DE ACONTECER O MILAGRE DA VIDA\"=paste(Milagre,\"%\",sep=\"\"),\"BOM CHURRASCO!!\"))}\n    \n    \n  \nif(divisao==\"por sexo\"){\nreturn(list(\"valor total\"=valortotal, \"Valor para cada homem\"=valorhp, \"valor para cada mulher\"=valormp,\"quantidade de cerveja(latas)\"=Qcerveja, \"quantidade de refrigerante\"=Refri, \"quantidade de suco\"=suco, \"quantidade de carne de boi (kg)\"=Qboi,\"quantidade de linguiça (kg)\"=Qlinguica, \"quantidade de queijo (kg)\"=Qqueijo, \"CHANCE DE ACONTECER O MILAGRE DA VIDA\"=paste(Milagre,\"%\",sep=\"\"),\"BOM CHURRASCO!!\"))}\n\nif(divisao==\"por bebida\"){ \n  return(list(\"valor total\"=valortotal, \"valor para quem nao bebe\"=valornaobebe,\"valor para quem bebe\"= valorcombebe,\"valor pessoas que levarão sua própria bebida\"=valorsembebe,\"quantidade de cerveja(latas)\"=Qcerveja,\"quantidade de refrigerante\"=Refri, \"quantidade de suco\"=suco, \"quantidade de carne de boi (kg)\"=Qboi,\"quantidade de linguiça (kg)\"=Qlinguica, \"quantidade de queijo (kg)\"=Qqueijo, \"CHANCE DE ACONTECER O MILAGRE DA VIDA\"=paste(Milagre,\"%\",sep=\"\"),\"BOM CHURRASCO!!\"))}\n\n}   ",
